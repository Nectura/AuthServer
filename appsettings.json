{
  "Cors": {
    "ExposedHeaders": [ "X-User-Agent", "X-Grpc-Web", "content-type", "x-grpc-web", "x-user-agent", "grpc-status", "grpc-message", "authorization" ]
  },
  "JwtAuth": {
    "ValidateAudience": true,
    "ValidateIssuer": true
  },
  "SocialAuth":{
    "SocialAuthRequestTimeoutInSeconds": 30
  },
  "LocalAuth": {
    "TimeoutInMilliseconds": 5000
  },
  "SpotifyAuth": {
    "AccessTokenExchangeEndpoint": "https://accounts.spotify.com/api/token",
    "UserMeEndpoint": "https://api.spotify.com/v1/me",
    "TimeoutInMilliseconds": 5000
  },
  "GoogleAuth": {
    "TimeoutInMilliseconds": 5000
  },
  "DiscordAuth": {
    "TimeoutInMilliseconds": 5000
  },
  "TwitchAuth": {
    "UserAuthorizationEndpoint": "https://id.twitch.tv/oauth2/authorize",
    "AccessTokenExchangeEndpoint": "https://id.twitch.tv/oauth2/token",
    "UserInfoEndpoint": "https://id.twitch.tv/oauth2/userinfo",
    "MinimalAuthScopes": [
      "user:read:email"
    ],
    "ExtendedAuthScopes": [
      "clips:edit",
      "channel:edit:commercial",
      "channel:manage:broadcast",
      "channel:manage:redemptions",
      "channel:read:redemptions",
      "channel:read:subscriptions",
      "moderation:read",
      "user:read:email",
      "user:edit:broadcast",
      "bits:read"
    ],
    "TimeoutInMilliseconds": 5000,
    "ForceVerify": true
  }
}
